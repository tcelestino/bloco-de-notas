---
layout: default
---
<main>
  <ol reversed class="post-list">
    {%- for post in collections.posts -%}
      <li>
        <span class="post-meta">{{ post.date | readableDate }}</span>
        <h2>
          <a class="post-link" href="{{ post.url | url }}">{{ post.data.title | safe }}</a>
        </h2>
      </li>
    {%- endfor -%}
  </ol>
</main>
